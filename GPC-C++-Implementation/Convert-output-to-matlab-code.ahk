#SingleInstance Force#NoEnv#InstallKeybdHook#InstallMouseHook#PersistentSetDefaultMouseSpeed,0ReturnNumpad0::	content := Clipboard	content := RegExReplace(content, "s)E:.*")	content := "a = [" RegExReplace(content, "[\r\n]+")	content := RegExReplace(content, ";$", "]';") "`nclf; plot(a(4,:)); hold on;plot(a(2,:))"	Clipboard := content	Return~Pause::$~#ScrollLock::	Suspend	Return$~#Esc::	WinGetActiveTitle, OutputVar	If InStr(OutputVar, A_ScriptName)		Reload	Return$~#^Esc::	ExitApp	Return